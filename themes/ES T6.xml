&lt;script src=&#34;https://multipurpose-website-builder/assets/js/variants.js&#34;&gt;&lt;/script&gt;
&lt;script src=&#34;https://multipurpose-website-builder/assets/js/i18n.js&#34;&gt;&lt;/script&gt;
&lt;script src=&#34;https://multipurpose-website-builder/assets/js/cart.js&#34;&gt;&lt;/script&gt;
&lt;script src=&#34;https://multipurpose-website-builder/assets/js/wishlist.js&#34;&gt;&lt;/script&gt;
&lt;script src=&#34;https://multipurpose-website-builder/assets/js/popup.js&#34;&gt;&lt;/script&gt;
&lt;script src=&#34;https://multipurpose-website-builder/assets/js/currency.js&#34;&gt;&lt;/script&gt;
&lt;script src=&#34;https://multipurpose-website-builder/assets/js/lazyload.js&#34;&gt;&lt;/script&gt;
&lt;script src=&#34;https://multipurpose-website-builder/assets/js/analytics.js&#34;&gt;&lt;/script&gt;
&lt;script src=&#34;https://multipurpose-website-builder/assets/js/seo.js&#34;&gt;&lt;/script&gt;
&lt;script src=&#34;https://multipurpose-website-builder/assets/js/gallery.js&#34;&gt;&lt;/script&gt;
  </body>
