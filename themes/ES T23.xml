    <script>
      (function () {
        function setFooterYear() {
          var el = document.getElementById('footerYear');
          if (el) el.textContent = String(new Date().getFullYear());
        }

        
        function fixWhatsappHref() {
          var a = document.getElementById('floatingWhatsapp');
          if (!a) return;
          try {
            
            var num = (window.Runtime &amp;&amp; Runtime.settings &amp;&amp; Runtime.settings.contact_whatsapp) ? String(Runtime.settings.contact_whatsapp) : '';
            num = (num || '').replace(/[^\d+]/g, '');
            if (num) a.setAttribute('href', 'https://wa.me/' + num);
          } catch (e) {}
        }

        document.addEventListener('DOMContentLoaded', function () {
          setFooterYear();
        });

        document.addEventListener('runtime_ready', function () {
          
          fixWhatsappHref();
          setFooterYear();
        });
      })();
    </script>

