    <section id='contact-section'>
      <section class='py-5 bg-light' id='contact'>
        <div class='container'>

          <h2 class='h3 mb-3' data-i18n='TEXT_CONTACT_US'></h2>
          <p class='text-muted' data-i18n='TEXT_CONTACT_INTRO'></p>

          <div class='row g-4'>
            <div class='col-lg-5'>
              <ul class='list-unstyled small'>
                <li>
                  <strong data-i18n='TEXT_PHONE'></strong>:
                  <span data-setting='contact_phone'></span>
                </li>
                <li>
                  <strong data-i18n='TEXT_EMAIL'></strong>:
                  <span data-setting='contact_email'></span>
                </li>
                <li>
                  <strong data-i18n='TEXT_WHATSAPP'></strong>:
                  <span data-setting='contact_whatsapp'></span>
                </li>
              </ul>
            </div>

            <div class='col-lg-7'>
              <form id='contactForm' novalidate='novalidate'>
                <div class='mb-3'>
                  <label class='form-label' data-i18n='TEXT_NAME' for='contactName'></label>
                  <input class='form-control' id='contactName' required='required' type='text'/>
                </div>

                <div class='mb-3'>
                  <label class='form-label' data-i18n='TEXT_EMAIL' for='contactEmail'></label>
                  <input class='form-control' id='contactEmail' required='required' type='email'/>
                </div>

                <div class='mb-3'>
                  <label class='form-label' data-i18n='TEXT_MESSAGE' for='contactMessage'></label>
                  <textarea class='form-control' id='contactMessage' required='required' rows='4'></textarea>
                </div>

                <div class='d-none'>
                  <input autocomplete='off' id='contactHp' type='text'/>
                </div>

                <button class='btn btn-primary mt-2' id='contactSubmit' type='submit'>
                  <span data-i18n='TEXT_SEND_MESSAGE'></span>
                </button>

                <div class='mt-2 small text-muted' id='contactStatus'></div>
              </form>
            </div>
          </div>

        </div>
      </section>
