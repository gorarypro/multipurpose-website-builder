<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html b:version='2' expr:dir='data:blog.languageDirection' 
      xmlns='http://www.w3.org/1999/xhtml' 
      xmlns:b='http://www.google.com/2005/gml/b' 
      xmlns:data='http://www.google.com/2005/gml/data' 
      xmlns:expr='http://www.google.com/2005/gml/expr'>


  <head>
    <meta charset='utf-8'/>
    <title>My Website</title>
    <meta content='width=device-width, initial-scale=1' name='viewport'/>

    
            
    
    <link href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css?v=1766717377160' rel='stylesheet'/>
    <link href='https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.css?v=1766717377160' rel='stylesheet'/>
    <link href='https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500:600:700&amp;display=swap' rel='stylesheet'/>
    
    
    <link href='https://cdn.jsdelivr.net/gh/gorarypro/multipurpose-website-builder@main/multipurpose-website-builder/src/assets/css/style.css?v=1766717377160' rel='stylesheet' type='text/css'/>  
    
    
    <b:skin>
      :root { 
        --fusion-primary: #000000; 
        --hero-bg: url("");
      }
      body { background-color: #f9fafb; font-family: 'Inter', sans-serif; }
      #navbar-iframe-container { display: none !important; }
      .btn-primary { background: var(--fusion-primary) !important; border-color: var(--fusion-primary) !important; }

      /* --- SIDEBAR &amp; MODALS --- */
      .sidebar-overlay { position: fixed; inset: 0; background: rgba(0,0,0,0.5); z-index: 1040; display: none; opacity: 0; transition: opacity 0.3s; }
      .sidebar-overlay.active { display: block; opacity: 1; }
      
      .sidebar { position: fixed; top: 0; right: -100%; width: 100%; max-width: 400px; height: 100%; background: white; transition: right 0.3s ease-in-out; z-index: 1050; display: flex; flex-direction: column; box-shadow: -5px 0 15px rgba(0,0,0,0.1); }
      .sidebar.active { right: 0; }
      .sidebar-header { padding: 20px; border-bottom: 1px solid #eee; display: flex; justify-content: space-between; align-items: center; }
      .sidebar-body { flex: 1; overflow-y: auto; padding: 20px; }
      .sidebar-footer { padding: 20px; border-top: 1px solid #eee; background: #fff; }
      
      /* Phone Modal */
      .phone-modal { position: fixed; top: 0; left: 0; width: 100%; height: 100%; background: rgba(0,0,0,0.6); z-index: 1060; display: none; align-items: center; justify-content: center; opacity: 0; transition: opacity 0.3s; }
      .phone-modal.active { display: flex; opacity: 1; }
      .phone-modal-content { background: white; width: 90%; max-width: 500px; border-radius: 12px; padding: 24px; box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.1); }
      
      /* QuickView Modal */
      .quick-view-modal { position: fixed; top: 0; left: 0; width: 100%; height: 100%; background: rgba(0,0,0,0.6); z-index: 1060; display: none; align-items: center; justify-content: center; opacity: 0; transition: opacity 0.3s; }
      .quick-view-modal.active { display: flex; opacity: 1; }
      .quick-view-modal-content { background: white; width: 90%; max-width: 800px; border-radius: 12px; overflow: hidden; display: flex; flex-direction: column; max-height: 90vh; }
      .quick-view-modal-header { padding: 15px; border-bottom: 1px solid #eee; display: flex; justify-content: space-between; align-items: center; }
      .quick-view-modal-body { overflow-y: auto; padding: 20px; }
      .quick-view-details { display: flex; gap: 20px; flex-wrap: wrap; }
      .quick-view-image { flex: 1; min-width: 250px; }
      .quick-view-image img { width: 100%; border-radius: 8px; object-fit: cover; }
      .quick-view-info { flex: 1; min-width: 250px; display: flex; flex-direction: column; justify-content: center; }

      /* --- FLOATING BUTTONS --- */
      .floating-btn { position: fixed; bottom: 20px; width: 56px; height: 56px; border-radius: 50%; display: flex; align-items: center; justify-content: center; box-shadow: 0 4px 6px rgba(0,0,0,0.2); z-index: 1000; text-decoration: none; transition: transform 0.2s; cursor: pointer; }
      .floating-btn:hover { transform: scale(1.1); }
      .floating-btn i { font-size: 1.5rem; color: #fff; }
      .floating-cart { right: 20px; background: #333; }
      .floating-wishlist { right: 85px; background: #e91e63; }
      .floating-whatsapp { left: 20px; background: #25D366; }
      .icon-count { position: absolute; top: -2px; right: -2px; background: #ff0000; color: white; font-size: 10px; width: 18px; height: 18px; border-radius: 50%; display: flex; align-items: center; justify-content: center; font-weight: bold; border: 2px solid white; }

      /* --- HERO &amp; LAYOUT --- */
      header#hero { background-size: cover; background-position: center; }
      .nav-icon { position: relative; }

      /* --- PRODUCT GRID --- */
      .product-card { border: 1px solid #eee; border-radius: 12px; overflow: hidden; transition: transform 0.2s, box-shadow 0.2s; background: #fff; }
      .product-card:hover { transform: translateY(-5px); box-shadow: 0 10px 15px rgba(0,0,0,0.1); }
      .product-img { width: 100%; aspect-ratio: 4/3; object-fit: cover; }
      .product-body { padding: 15px; }

      /* --- FORMS --- */
      .info-option { margin-top: 15px; margin-bottom: 15px; font-size: 0.9rem; color: #666; }
      .info-options { display: flex; gap: 10px; margin-top: 10px; }
      .info-options button { flex: 1; padding: 8px; border-radius: 6px; border: 1px solid #ddd; background: #fff; cursor: pointer; }
      .info-options button.btn-primary { background: var(--fusion-primary); color: white; border: none; }
      
    </b:skin>

    
    
    <script type="text/javascript">
        window.FUSION_CONFIG = {
            settings: {
                primary_color: "#000000",
                currency_symbol: "https:\/\/cdn.jsdelivr.net\/gh\/gorarypro\/multipurpose-website-builder@main\/multipurpose-website-builder\/src\/assets\/",
                contact_whatsapp: ""
            }
        };
    </script>
  </head>
  
  <body>
  
      
      
      
      <div class="toast align-items-center text-white bg-success border-0" id="notificationToast" role="alert">
        <div class="d-flex">
          <div class="toast-body"></div>
          <button aria-label="Close" class="btn-close btn-close-white me-2 m-auto" data-bs-dismiss="toast" type="button"></button>
        </div>
      </div>

      <div class="sidebar-overlay" id="sidebarOverlay"></div>

      
      <div class="sidebar" id="cartSidebar">
        <div class="sidebar-header">
          <h4 class="m-0">Your Cart</h4>
          <i class="bi bi-x-lg" id="closeCart" style="cursor:pointer" />
        </div>
        <div class="sidebar-body" id="cartBody">
          <div class="empty-sidebar text-center text-muted py-5" id="emptyCartMessage">
            <i class="bi bi-cart-x" style="font-size: 2rem; display: block; margin-bottom: 10px;" />
            <p>Your cart is empty</p>
          </div>
        </div>
        <div class="sidebar-footer" id="cartFooter" style="display: none;">
          <div class="d-flex justify-content-between mb-3 fw-bold fs-5">
            <span>Total:</span>
            <span id="cartTotal">0 https://cdn.jsdelivr.net/gh/gorarypro/multipurpose-website-builder@main/multipurpose-website-builder/src/assets/</span>
          </div>
          <button class="btn btn-primary w-100 py-2 rounded-pill checkout-btn" id="checkoutBtn">Proceed to Checkout</button>
        </div>
      </div>

      
      <div class="sidebar" id="wishlistSidebar">
        <div class="sidebar-header">
          <h4 class="m-0">Your Wishlist</h4>
          <i class="bi bi-x-lg" id="closeWishlist" style="cursor:pointer" />
        </div>
        <div class="sidebar-body" id="wishlistBody">
          <div class="empty-sidebar text-center text-muted py-5" id="emptyWishlistMessage">
            <i class="bi bi-heartbreak" style="font-size: 2rem; display: block; margin-bottom: 10px;" />
            <p>Your wishlist is empty</p>
          </div>
        </div>
      </div>

      
      <div class="phone-modal" id="phoneModal">
        <div class="phone-modal-content">
          <div class="phone-modal-header mb-4">
            <h4 class="m-0">Complete Your Order</h4>
          </div>
          <div class="phone-modal-body">
            <div class="mb-3">
              <label class="form-label" for="customerPhone">Phone Number *</label>
              <input class="form-control" id="customerPhone" placeholder="" required="required" type="tel" />
            </div>
            <div class="info-option">
              <p>Would you like to provide additional information?</p>
              <div class="info-options">
                <button class="btn-secondary" id="noThanksBtn">No, thanks</button>
                <button class="btn-primary" id="addInfoBtn">Yes, add more info</button>
              </div>
            </div>
            <div id="additionalInfo" style="display:none; margin-top: 15px;">
              <div class="mb-3">
                <label class="form-label" for="customerName">Full Name <span class="text-muted fw-light">(Optional)</span></label>
                <input class="form-control" id="customerName" placeholder="John Doe" type="text" />
              </div>
              <div class="mb-3">
                <label class="form-label" for="customerAddress">Delivery Address <span class="text-muted fw-light">(Optional)</span></label>
                <textarea class="form-control" id="customerAddress" placeholder="123 Street, City" rows="3"></textarea>
              </div>
            </div>
          </div>
          <div class="d-flex gap-2 mt-4">
            <button class="btn btn-light flex-fill btn-cancel" id="cancelCheckout">Cancel</button>
            <button class="btn btn-success flex-fill btn-success fw-bold confirmCheckout" id="confirmCheckout">
              <span id="checkoutButtonText">Place Order</span>
              <div class="spinner-border spinner-border-sm d-none" id="checkoutLoading"></div>
            </button>
          </div>
        </div>
      </div>

      
      <div class="quick-view-modal" id="quickViewModal">
        <div class="quick-view-modal-content">
          <div class="quick-view-modal-header">
            <h4 id="quickViewTitle" class="m-0">Product Title</h4>
            <button class="btn-close" id="closeQuickView" type="button"><i class="bi bi-x" /></button>
          </div>
          <div class="quick-view-modal-body">
            <div class="quick-view-details">
              <div class="quick-view-image">
                <img src="https://placehold.co/600x400" id="quickViewImage" alt="Product Image" />
              </div>
              <div class="quick-view-info">
                <div class="price fs-4 fw-bold mb-3" id="quickViewPrice">0 https://cdn.jsdelivr.net/gh/gorarypro/multipurpose-website-builder@main/multipurpose-website-builder/src/assets/</div>
                <p class="text-muted mb-4" id="quickViewDescription">Loading product details...</p>
                <button class="btn btn-primary w-100 py-3 order-btn add-to-cart-btn" id="quickViewAddToCartBtn">
                  <i class="bi bi-cart-plus me-2" /> Add to Cart
                </button>
              </div>
            </div>
          </div>
        </div>
      </div>

      
      <div id="marketingPopup" style="display:none; position: fixed; top: 50%; left: 50%; transform: translate(-50%, -50%); z-index: 9999; background: white; padding: 20px; border-radius: 15px; box-shadow: 0 10px 25px rgba(0,0,0,0.2); max-width: 90%;">
        <div style="position: absolute; top: 10px; right: 10px; cursor: pointer;" onclick="this.parentElement.style.display='none'"><i class="bi bi-x-lg" /></div>
        <div id="popupContentArea"></div>
      </div>

      
      <div class="floating-btn floating-cart" id="floatingCart">
        <i class="bi bi-cart3" />
        <span class="icon-count" id="floatingCartCount">0</span>
      </div>
      <div class="floating-btn floating-wishlist" id="floatingWishlist">
        <i class="bi bi-heart-fill" />
        <span class="icon-count" id="floatingWishlistCount">0</span>
      </div>
      <a class="floating-btn floating-whatsapp" href="https://wa.me/" id="floatingWhatsapp" target="_blank">
        <i class="bi bi-whatsapp" />
      </a>

      
      <header id="hero" style='background: linear-gradient(rgba(0,0,0,0.5), rgba(0,0,0,0.5)), var(--hero-bg) center/cover; padding: 100px 0; color: #fff; text-align: center;'>
        <div class="container">
          <h1 class="display-4 fw-bold"></h1>
          <p class="lead mb-4">Experience the finest flavors in Casablanca.</p>
          <a class="btn btn-primary btn-lg px-5 rounded-pill" href="#menu">Shop Now</a>
        </div>
      </header>

      
      <nav class="navbar navbar-expand-lg navbar-light bg-white sticky-top shadow-sm">
        <div class="container-fluid">
          <a class="navbar-brand fw-bold text-primary" href="#"></a>
          <button aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation" class="navbar-toggler" data-bs-target="#navbarNav" data-bs-toggle="collapse" type="button">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav ms-auto">
              <li class="nav-item"><a class="nav-link" href="#hero">Home</a></li>
              <li class="nav-item"><a class="nav-link" href="#menu">Menu</a></li>
              <li class="nav-item">
                <a class="nav-link nav-icon" id="wishlistIcon">
                  <i class="bi bi-heart" />
                  <span class="icon-count wishlist-count" id="wishlistCount">0</span>
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link nav-icon" id="cartIcon">
                  <i class="bi bi-cart3" />
                  <span class="icon-count" id="cartCount">0</span>
                </a>
              </li>
            </ul>
          </div>
        </div>
      </nav>

      
      <section id="menu">
        <div class="container py-5">
          <h2 class="fw-bold mb-4">Our Menu</h2>
          
          <div class="filter-container mb-4" id="filterContainer">
            <button class="btn btn-sm btn-outline-dark active me-2 rounded-pill px-3 filter-btn" data-filter="all">All</button>
          </div>
          
          <div class="loading-container text-center py-5 d-none" id="loadingContainer">
            <div class="spinner-border text-primary" role="status"></div>
            <div class="mt-2 text-muted">Loading products...</div>
          </div>
          
          <div class="error-container text-center py-5 d-none" id="errorContainer">
            <h3 class="h4 text-danger">Unable to Load Menu</h3>
            <p>Please check your internet connection and API settings.</p>
          </div>
          
          
          <div id="menuSections" class="row g-4">
            &lt;div class=&#34;col-12 text-center py-5&#34;&gt;&lt;p class=&#34;text-muted&#34;&gt;No products found.&lt;/p&gt;&lt;/div&gt;          </div>
        </div>
      </section>

      <footer class="bg-dark text-white py-4 mt-5 text-center">
        <div class="container">
          <p class="mb-0">&copy; 2025 . All rights reserved.</p>
        </div>
      </footer>
    
    <b:section class='main' id='main-widget-area' showaddelement='no'/>
    
    
    <script src='https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js'/>
    
